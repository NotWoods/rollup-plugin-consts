{
    "name": "rollup-plugin-consts",
    "version": "1.2.0",
    "description": "Import build time constants with Rollup",
    "main": "dist/rollup-plugin-consts.cjs",
    "module": "dist/rollup-plugin-consts.mjs",
    "types": "index.d.ts",
    "exports": {
        "import": "./dist/rollup-plugin-consts.mjs",
        "require": "./dist/rollup-plugin-consts.cjs",
        "types": "./index.d.ts"
    },
    "files": [
        "dist",
        "index.d.ts",
        "consts.d.ts"
    ],
    "scripts": {
        "build": "rollup -c",
        "pretest": "npm run build",
        "test": "jest && prettier src test .github \"*.{js,ts,mjs,md}\" --check",
        "format": "prettier src test .github \"*.{js,ts,mjs,md}\" --write"
    },
    "repository": "NotWoods/rollup-plugin-consts",
    "bugs": "https://github.com/NotWoods/rollup-plugin-consts/issues",
    "homepage": "https://github.com/NotWoods/rollup-plugin-consts#readme",
    "keywords": [
        "rollup",
        "rollup-plugin",
        "const",
        "consts",
        "constant"
    ],
    "author": "Jake Archibald",
    "contributors": [
        "Tiger Oakes <contact@tigeroakes.com> (https://tigeroakes.com)"
    ],
    "license": "Apache-2.0",
    "peerDependencies": {
        "rollup": ">=1.15.0 <4"
    },
    "devDependencies": {
        "@types/jest": "^29.1.2",
        "jest": "^29.2.0",
        "prettier": "~3.0.1",
        "rollup": "^3.2.0",
        "typescript": "^5.1.6"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "es5",
        "proseWrap": "always"
    }
}
